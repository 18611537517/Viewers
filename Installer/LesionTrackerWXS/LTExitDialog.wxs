<?xml version="1.0" encoding="utf-8"?>
<Wix xmlns="http://schemas.microsoft.com/wix/2006/wi">
    <Fragment>
        <UI>
            <Dialog Id="LTExitDialog" Width="370" Height="270" Title="!(loc.InstallDirDlg_Title)">
                <Control Id="BannerBitmap" Type="Bitmap" X="0" Y="0" Width="370" Height="44" TabSkip="no" Text="!(loc.InstallDirDlgBannerBitmap)" />
				<Control Id="Title" Type="Text" X="15" Y="14" Width="200" Height="15" Transparent="yes" NoPrefix="yes" Text="{\WixUI_Font_Title}Upload/View Studies" />
                <Control Id="BannerLine" Type="Line" X="0" Y="44" Width="370" Height="0" />

                <Control Id="OrthancServerLink" Height="20" Width="370" Type="Hyperlink" X="15" Y="60"> 
					<Text>{\WixUI_Font_Big}You can upload studies here: <![CDATA[<a href="http://localhost:8042">http://localhost:8042</a>]]></Text> 
				</Control>
				
				<Control Id="LTViewerLink" Height="20" Width="370" Type="Hyperlink" X="15" Y="80"> 
					<Text>{\WixUI_Font_Big}You can view studies here: <![CDATA[<a href="http://localhost:3000">http://localhost:3000</a>]]></Text> 
				</Control>
				
				<Control Id="BottomLine" Type="Line" X="0" Y="234" Width="370" Height="0" />

				<Control Id="Done" Type="PushButton" X="304" Y="243" Width="56" Height="17" Default="yes" Text="Done" />
            </Dialog>
        </UI>
    </Fragment>
</Wix>